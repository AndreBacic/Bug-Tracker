@model ManageOrganizationModel

@{
    ViewData["Title"] = $"Manage {Model.Organization.Name}";
}
<form asp-action="ManageOrganization">
    <div asp-validation-summary="ModelOnly" class=""></div>
    <div class="form-group">
        <label asp-for="DidUpdateOrg" class="control-label"></label>
        <input asp-for="DidUpdateOrg" class="form-control" />
        <p asp-validation-for="DidUpdateOrg" class=""></p>
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
<div>
    <a asp-action="OrganizationHome" asp-controller="Organization">Back to Organization Home</a>
</div>